<IfModule mod_rewrite.c>
   RewriteEngine On
   RewriteRule ^([a-zA-Z0-9_-]+)/?$ /~joonaoko/ringbanan/index.php?osio=$1
   RewriteRule ^([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/?$ /~joonaoko/ringbanan/index.php?osio=$1&uutisid=$2
   RewriteRule ^([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/?$ /~joonaoko/ringbanan/index.php?osio=$1&uutisid=$2&toiminto=$3
</IfModule>
